#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[])
{
    int A[]={4,-3,2,1,6,-1,-2,4};
    int boyut=8;
    int k,l;
    for(k=0;k<(boyut/2);k++){
     printf("%d  ",A[k]);
    }
     printf("\n");
    for(l=4;l<boyut;l++){
     printf("%d  ",A[l]);
    }
    
  system("PAUSE");	
  return 0;
}
